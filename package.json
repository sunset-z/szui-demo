{
  "name": "@thurs/szui-demo",
  "version": "0.1.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunset-z/szui-demo.git"
  },
  "license": "MIT",
  "main": "es/index.js",
  "files": [
    "es",
    "typings"
  ],
  "types": "typings/index.d.ts",
  "dependencies": {
    "@emotion/core": "10.1.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "classnames": "^2.5.1",
    "craco-less": "^3.0.1",
    "docz": "^2.4.0",
    "gatsby-plugin-less": "^7.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "set BROWSER=none&&craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "prettier": "prettier --write .",
    "prepare": "husky",
    "docz:dev": "docz dev",
    "docz:build:local": "docz build --config doczrc.local.js",
    "docz:build": "docz build",
    "build:css": "node scripts/build-less.js",
    "build:ts": "node scripts/build-ts.js",
    "build:es": "babel src/components --out-dir dist/es --extensions \".ts,.tsx,.js,.jsx\" --ignore \".mdx\"",
    "build:npm": "yarn build:css && yarn build:ts && yarn build:es",
    "prepack": "cp -r dist/* .",
    "postpack": "rm -rf es typings",
    "clean": "rm es && rm typings"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "husky": "^9.0.11",
    "prettier": "3.3.2",
    "tscpaths": "^0.0.9"
  }
}
